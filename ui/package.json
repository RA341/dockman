{
  "name": "ui",
  "version": "0.0.0",
  "description": "frontend for dockman",
  "private": true,
  "author": "ra341",
  "main": "electron.cjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "desk:dev": "cross-env NODE_ENV=development electron .",
    "desk:build": "npx electron-builder --dir"
  },
  "build": {
    "appId": "dev.radn.dockman",
    "files": [
      "electron.cjs"
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "icon": "public/dockman.png"
    },
    "linux": {
      "icon": "public/dockman.png"
    }
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.10.1",
    "@connectrpc/connect": "^2.0.2",
    "@connectrpc/connect-web": "^2.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.6",
    "@mui/material": "^7.3.6",
    "@mui/styled-engine-sc": "^7.3.6",
    "@nivo/line": "^0.99.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/addon-search": "^0.15.0",
    "@xterm/xterm": "^5.5.0",
    "composerize": "^1.7.5",
    "immer": "^11.1.0",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.10.1",
    "reconnecting-websocket": "^4.4.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.19",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.49.0",
    "vite": "7.1.11",
    "wait-on": "^9.0.3"
  }
